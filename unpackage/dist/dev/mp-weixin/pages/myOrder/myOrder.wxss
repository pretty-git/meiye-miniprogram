  width: 15%;
  color: #d43030;
}
.del-btn {
  padding: 10rpx 0 10rpx 36rpx;
}
.id_image {
  width: 29px;
  height: 29px;
  border-radius: 4px;
}
.id_image image {
  width: 29px;
  height: 29px;
  border-radius: 4px;
}
.itemcondition .id_image1 {
  width: 29px;
